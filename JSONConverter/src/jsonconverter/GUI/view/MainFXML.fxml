                              <TableColumn fx:id="pauseButtonColumn" prefWidth="65.0" />
                          </columns>
                        </TableView>
                        <Button layoutX="227.0" layoutY="513.0" mnemonicParsing="false" onAction="#chooseDirectoryButtonClick" text="Choose Directory" />
