                        <Button layoutX="588.0" layoutY="512.0" mnemonicParsing="false" onAction="#pauseTasksButtonClick" prefHeight="27.0" prefWidth="138.0" text="PAUSE PROCESS" />
                        <Button layoutX="745.0" layoutY="513.0" mnemonicParsing="false" onAction="#deleteTasksButtonClick" prefHeight="27.0" prefWidth="138.0" text="DELETE PROCESS" />
                        <ChoiceBox fx:id="configChoiceBox" layoutX="20.0" layoutY="129.0" prefWidth="150.0" />


                        <Label fx:id="labelFileExtension" layoutX="109.0" layoutY="34.0" text=".XXX">
