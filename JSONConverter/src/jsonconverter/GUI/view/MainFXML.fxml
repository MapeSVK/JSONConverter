                        <Button layoutX="20.0" layoutY="72.0" mnemonicParsing="false" onAction="#importFileButtonClick" prefHeight="13.0" prefWidth="59.0" text="Import" />
                        <Button layoutX="21.0" layoutY="168.0" mnemonicParsing="false" onAction="#createNewConfigButtonClick" text="Create new" />
