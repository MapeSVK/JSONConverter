                        <TableView fx:id="tasksTableView" layoutX="202.0" layoutY="39.0" prefHeight="459.0" prefWidth="705.0">
                          <columns>
                              <TableColumn fx:id="extensionColumn" prefWidth="44.0" />
                            <TableColumn fx:id="nameOfTheFileColumn" prefWidth="223.0" text="NAME" />
                            <TableColumn fx:id="configNameColumn" prefWidth="245.0" text="CONGIF NAME" />
